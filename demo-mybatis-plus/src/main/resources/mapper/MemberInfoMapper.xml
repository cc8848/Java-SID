<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quaint.demo.mybatis.plus.mapper.DemoUserMapper">

    <select id="getUserByName" resultType="com.quaint.demo.mybatis.plus.po.DemoUserPO">
        select * from
          demo_user
        where
          name = #{name}
        limit 1
    </select>


</mapper>
